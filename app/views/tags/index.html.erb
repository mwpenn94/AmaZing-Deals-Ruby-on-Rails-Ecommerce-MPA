<h1> Tags </h1>
<% if current_user.support? %>
    <p><strong><%= link_to "New tag", new_tag_path(@tag) %></strong></p>
<% end %>
  <div>
<% @tags.each do |tag| %>
    <div>
        <h2><%= tag.name %></h2> <p><%= button_to "Delete", tag_path(tag), method: :delete, data: {confirm: "Are you sure?"} %></p>
    </div>
<% end %>